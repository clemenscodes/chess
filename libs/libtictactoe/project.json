{
	"name": "libtictactoe",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"projectType": "library",
	"sourceRoot": "libs/libtictactoe/src",
	"targets": {
		"build": {
			"executor": "@jnxplus/nx-maven:run-task",
			"outputs": ["{projectRoot}/target"],
			"options": {
				"task": "install -DskipTests=true"
			}
		},
		"test": {
			"executor": "@jnxplus/nx-maven:run-task",
			"options": {
				"task": "test"
			}
		},
		"docs": {
			"command": "firefox docs/{projectRoot}/index.html"
		},
		"coverage": {
			"command": "firefox coverage/{projectRoot}/index.html"
		}
	},
	"tags": []
}
